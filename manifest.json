{
  "manifest_version": 3,
  "name": "Umair Dynamic Redirector",
  "version": "1.1",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "alarms",
    "storage",
    "idle"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}